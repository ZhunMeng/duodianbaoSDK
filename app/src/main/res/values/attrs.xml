<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="AdmoreDriftView">
        <attr name="admore_animate" format="boolean" />
        <attr name="admore_animateDuration" format="integer" />
        <attr name="admore_closeBtnWidth" format="dimension" />
        <attr name="admore_closeBtnHeight" format="dimension" />
        <attr name="admore_shape" format="enum">
            <enum name="circular" value="0" />
            <enum name="rectangle" value="1" />
        </attr>
    </declare-styleable>


</resources>
